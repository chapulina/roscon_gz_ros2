        <keyframe type='cam_pose' pose='-2615.004395 989.036499 -33.712574 0 -0.017604 -0.124844'
            text='<h2>Gazebo ROS camera plugin demo</h2>

<h3>Check camera info</h3>

<p style="color: #222255; font-family: monospace">
ros2 topic echo /gazebo_ros_camera/camera/camera_info
</p>

<h3>Check camera images</h3>

<p style="color: #222255; font-family: monospace">
ros2 run rqt_image_view rqt_image_view /gazebo_ros_camera/camera/image_raw
</p>

<h3>Sensor SDF</h3>

<pre style="color: #222255; font-family: monospace">
<<sensor type="camera" name="camera">>
  <<update_rate>>5<</update_rate>>
  <<visualize>>true<</visualize>>
  <<camera name="camera">>
    <<horizontal_fov>>0.927295218<</horizontal_fov>>
    <<image>>
      <<width>>640<</width>>
      <<height>>480<</height>>
      <<format>>R8G8B8<</format>>
    <</image>>
    <<clip>>
      <<near>>0.02<</near>>
      <<far>>300<</far>>
    <</clip>>
    <<distortion>>
      <<k1>>-0.1<</k1>>
      <<k2>>-0.1<</k2>>
      <<p1>>0<</p1>>
      <<p2>>0<</p2>>
      <<k3>>-0.1<</k3>>
      <<center>>0.5 0.5<</center>>
    <</distortion>>
  <</camera>>
  <<plugin name="camera_controller" filename="libgazebo_ros_camera.so">>
    <<border_crop>>false<</border_crop>>
  <</plugin>>
<</sensor>>
</pre>
'/>

